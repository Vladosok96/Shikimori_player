{
  "name": "Shikimori_player",
  "description": "Better player for shikimori.",
  "version": "1.0.1",
  "manifest_version": 3,
  "host_permissions": ["https://*.shiki.one/*"],
  "content_scripts": [{
    "css": ["inpage.css"],
    "js": ["content.js"],
    "matches": ["https://*.shiki.one/*"],
    "run_at": "document_start"
  }],
  "web_accessible_resources": [{
    "resources": ["inpage.js"],
    "matches": ["https://*.shiki.one/*"]
  }],
  "icons": {
    "128": "/128.png"
  }
}
