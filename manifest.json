{
  "name": "Shikimori_player",
  "description": "Better player for shikimori.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [ {
    "css": [ "inpage.css" ],
    "js": [ "content.js" ],
    "matches": [ "https://*.shikimori.me/*" ],
    "run_at": "document_end"
   } ],
  "web_accessible_resources": [ {
    "matches": [ "\u003Call_urls>" ],
    "resources": [ "inpage.js" ]
  } ]
}
